<!-- Paintings Content -->
<div id="painting">

  <%
  const paintings = JSON.parse(
    site.pages.data[0]._content.match(/#\+BEGIN_SRC json([^]*)#\+END_SRC/)[1]
  )
  %>

  <%- partial('_partial/intro') %>
  
  <div class="content">
    <% paintings.map(painting => { %>
      <div class="card">
        <img class="picture" alt="" src="<%= painting.src %>" />
        <p><%= painting.title %></p>
      </div>
    <% }) %>
  </div>

  <div class="photo-imager-container" style="display:none">
    <div class="image-container">
      <img alt="" src="" />      
    </div>
    <i data-feather="x"></i>
  </div>
</div>
