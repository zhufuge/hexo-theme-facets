<header>
  <div class="navbar navbar-default navbar-custom navbar-fixed-top" role="navigation">
    <div class="container-fluid">

      <div class="navbar-header">
        <a class="navbar-brand animated pulse" href="<%= config.root %>">
          <span class="brand-logo">
            <%= config.title %>
          </span>
          's Blog
        </a>
        <button type="button" class="navbar-toggle collapsed" data-toggle="collapse" data-target="#nav-list">
          <span class="sr-only">Toggle navigation</span>
          <span class="icon-bar"></span>
          <span class="icon-bar"></span>
          <span class="icon-bar"></span>
        </button>
      </div>

      <div id="nav-list" class="navbar-collapse collapse">
        <ul class="nav navbar-nav navbar-right">
          <% for (var i in theme.menu){ %>
            <li>
              <a class="main-nav-link" href="<%- url_for(theme.menu[i]) %>"><%= i %></a>
            </li>
          <% } %>
        </ul>
      </div>

    </div>
  </div>
</header>
